int main() {
    int i = 0;
    int sum = 0;
    while (i < 10) {
        i = i + 1;
        if (i == 5) {
            continue;
        }
        if (i > 8) {
            break;
        }
        sum = sum + i;
    }
   
    if (sum > 20 && sum < 40) {
        return 1;
    } else {
        return 0;
    }
}
